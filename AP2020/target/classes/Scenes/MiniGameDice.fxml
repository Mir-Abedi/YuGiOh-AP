<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.net.URL?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.graphics.duelgraphics.MiniGameDice">
   <children>
      <Button fx:id="playerButton" layoutX="139.0" layoutY="285.0" mnemonicParsing="false" style="-fx-background-radius: 10px;" text="Roll Dice !" />
