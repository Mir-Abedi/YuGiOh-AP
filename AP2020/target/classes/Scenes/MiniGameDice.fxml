      <ImageView fx:id="playerDice" fitHeight="150.0" fitWidth="200.0" layoutX="99.0" layoutY="69.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/dice none.png" />
         </image>
      </ImageView>
      <ImageView fx:id="rivalDice" fitHeight="150.0" fitWidth="200.0" layoutX="376.0" layoutY="69.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/dice none.png" />
         </image>
      </ImageView>
      <Label fx:id="playerName" layoutX="151.0" layoutY="35.0" textAlignment="CENTER" wrapText="true" />
      <Label fx:id="rivalName" layoutX="428.0" layoutY="35.0" textAlignment="CENTER" wrapText="true" />
      <Label fx:id="result" layoutX="286.0" layoutY="246.0" textAlignment="CENTER" wrapText="true" />
   </children>
   <stylesheets>
      <URL value="@StyleSheets/MiniGameMenus.css" />
